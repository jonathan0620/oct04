package 메서드연습;

import javax.swing.JOptionPane;

public class 반응여부테스트 {

	public static void main(String[] args) {
		int x = Integer.parseInt("100"); //100
//		String y = System.out.println(x); //void 됨 오류남
		String data = JOptionPane.showInputDialog("이름을 입력"); //void x
		//입력한 데이터를 가져다 여기 주기 때문
		JOptionPane.showMessageDialog(null, data); // void 이기때문에 넣을 수 없음
		//void인지 아닌지가 중요하다 
	}

}
